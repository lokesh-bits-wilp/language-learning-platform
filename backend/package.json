{
	"name": "language-learning-backend-service",
	"version": "1.0.0",
	"description": "language-learning-backend-service",
	"scripts": {
		"dev": "debug=* NODE_PATH=./src ts-node-dev --respawn ./src/index.ts",
		"build": "rimraf dist && tsc",
		"start": "nodemon --exec ts-node dist/index.js",
		"start:dev": "NODE_PATH=dist node dist/index.js",
		"pm2": "pm2-runtime ./dist/index.js",
		"set:pghost": "set PG_HOST=localhost",
		"set:nodepath": "set NODE_PATH=./src",
		"migration:create": "NODE_PATH=./src npm run typeorm migration:create ./src/orm/migrations/${migration_file_name}",
		"migration:run": "NODE_PATH=./src npm run typeorm migration:run -- -d ./src/orm/config/OrmConfig.ts",
		"typeorm": "typeorm-ts-node-commonjs"
	},
	"dependencies": {
		"@babel/preset-typescript": "^7.21.5",
		"@types/swagger-jsdoc": "^6.0.1",
		"@types/swagger-ui-express": "^4.1.3",
		"axios": "^1.4.0",
		"bcrypt": "^5.1.1",
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"express": "^4.17.3",
		"helmet": "^5.0.2",
		"jsonwebtoken": "^8.5.1",
		"keccak256": "^1.0.6",
		"merkletreejs": "^0.3.10",
		"morgan": "^1.10.0",
		"multer": "^1.4.5-lts.1",
		"node-input-validator": "^4.5.1",
		"nodemailer": "^6.9.7",
		"pg": "^8.7.3",
		"reflect-metadata": "^0.1.13",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^4.6.3",
		"typeorm": "^0.3.17",
		"typeorm-naming-strategies": "^4.1.0",
		"typeorm-seeding": "^1.6.1",
		"validator": "^13.7.0",
		"winston": "^3.8.2",
		"winston-daily-rotate-file": "^4.7.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.2.4",
		"@commitlint/config-conventional": "^16.2.4",
		"@types/bcrypt": "^5.0.0",
		"@types/bcryptjs": "^2.4.2",
		"@types/body-parser": "^1.19.2",
		"@types/chai": "^4.3.1",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.17",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/mocha": "^9.1.1",
		"@types/morgan": "^1.9.3",
		"@types/multer": "^1.4.7",
		"@types/node": "^17.0.33",
		"@types/supertest": "^2.0.12",
		"@types/uuid": "^9.0.1",
		"@types/validator": "^13.7.2",
		"@typescript-eslint/eslint-plugin": "^5.23.0",
		"@typescript-eslint/parser": "^5.23.0",
		"chai": "^4.3.6",
		"commitizen": "^4.2.4",
		"dotenv": "^16.0.1",
		"eslint": "^8.15.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-no-array-reduce": "^1.0.58",
